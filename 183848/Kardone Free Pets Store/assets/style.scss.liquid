	}
}

.icon-arrow-right,
.icon-arrow-left {
	height: 9px;
}
.linklist_menu_title {
	position: relative;
	padding-right: 0px;

	&:after {
		position: absolute;
		content: '\f107';
		font: 14px/1em 'fontAwesome';
		right: 5px;
		top: 50%;
		margin-top: -7px;
		cursor: pointer;
		display: none;
		@include transition(.3s,transform);
	}
	&.open:after {
		transform: rotate(180deg);
	}
}




