// 4.1. GENERAL STYLES, PARAGRAPHS, LINKS, ETC ======================================================================================================
html,body {
	overflow-x: hidden;
	font: #{$base_font_size}/1.5em $base_font;
	color: $base_font_color;
	background: $bg_color;
	font-weight: {{ settings.type_base_font.weight }};
	font-style: {{ settings.type_base_font.style }};
}
a {
	color: $links_color;
	text-decoration: none;
	@include transition(0.3s, background, border-color, color);
}

a:hover,
a:focus {
	color: $links_color_hover;
	outline: 0;
}
.row {
	overflow: hidden;
}
.container_big {
	max-width: 1810px;
	margin-left: auto;
	margin-right: auto;
}
#page_preloader__bg {
	background: #fff;position: fixed;top: 0;right: 0;bottom: 0;left: 0;z-index: 1000;
