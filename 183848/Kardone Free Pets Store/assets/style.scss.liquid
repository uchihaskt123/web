// 6.5. BREADCRUMBS =================================================================================================================================
.breadcrumbs {
	padding: 0 0 26px 0;
	ul {
		list-style: none;
		padding: 0;
		font-size: 0;
		line-height: 0;
	}

	li {
		display: inline-block;
		padding: 21px 8px 0 0;
		span {
			position: relative;
		}
	}
	li ~ li {}
	li ~ li:before {
		content: "|";
		margin: 0 10px 0 0;
		font-size: 17px;
		font-weight: lighter;
	}
	li:last-child {
		span:after {
			content: '';
			position: absolute;
			bottom: -1px;
			left: 0;
			width: 100%;
			height: 1px;
			opacity: 1;
			background: $base_font_color;
			@include transition(.2s,opacity);
		}
	}

	a,
	span {
		font-size: 14px;
		line-height: 1em;
		letter-spacing: -0.07em;
	}

	span {
		text-transform: capitalize;
	}
	a {
		position: relative;
		&:after {
			content: '';
			position: absolute;
			bottom: -1px;
			left: 0;
			width: 100%;
			height: 1px;
			opacity: 0;
			background: $links_color_hover;
			@include transition(.2s,opacity);
		}
		&:hover:after {
			opacity: 1;
		}
	}
}



// 6.6. BUTTON FILTERS ==============================================================================================================================
.button_filters {
	margin: 5px 0 30px;
