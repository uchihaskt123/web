#product_quick_view {
	width: 800px;
	.swiper_btn {
		color: $color_1;
		&:hover {
			color: $color_3;
			background: rgba(0, 0, 0, 0.3);
		}
	}
	.btn_prev {
		left: 0;
		&:after {
			content: "\f104";
			font: 24px/1em "FontAwesome";
			color: inherit;
			line-height: 28px;
		}
	}
	.btn_next {
		right: 0;
		&:after {
			content: "\f105";
			font: 24px/1em "FontAwesome";
			color: inherit;
			line-height: 28px;
		}
	}
}

.product_quick_wrapper {
	width: 100%;
	display: table;
}

.quick_view__left {
	width: 380px;
	padding: 0;
	position: relative;
	#img_big {
		width: 100%;
		line-height: 0;
		img {
			background: rgba(0, 0, 0, 0.05);
		}
	}
	.swiper-slide {
		display: inline-block;
		padding: 3px;
	}
	.product_images {
		margin-top: 10px;
	}
}

.quick_view__right {
	height: 100%;
	display: table-cell;
	overflow: hidden;
	padding: 0 0 0 38px;
	vertical-align: top;
	p {
		margin: 5px 0 0 0;
		font-size: 14px;
	}
	label {
		font-weight: bold;
		font-size: 14px;
		color: $color_4;
	}
	.quantity_input {
		height: 52px;
		border-radius: 5px;
		width: 100%;
	}
	#product_info_link {
		margin-top: 30px;
	}
}

#quick_view__name {
	margin: 0;
	font-size: 22px;
	letter-spacing: -0.01em;
}

#quick_view__type {}

#quick_view__vendor {}

#quick_view__variants {
	overflow: hidden;
	margin: 12px -10px 12px -10px;
	label {
		display: block;
		padding: 0 10px;
	}
	.options_wrapper {
	}
	.selector-wrapper {
		width: 33.33333333333333%;
		float: left;
		padding: 3px 10px 0 10px;
		label {
			display: none !important;
		}
		select {
			width: 100%;
			display: block;
			padding: 3px 6px;
			height: 30px;
		}
	}
}

#quick_view__price {
	margin: 22px 0 0 0;
	position: relative;
	.price {
		margin: 0;
		padding: 0 60px 0 0;
	}
	.money {
		margin: 0 7px 0 0;
		font-size: 24px;
		color: $heading_font_color;
	}
	.item_price {
		display: block;
		width: 100%;
		font-weight: bold;
		font-size: 30px;
		letter-spacing: -0.04em;
		margin-bottom: 8px;
	}
	.money_sale {
		font-size: 16px;
	}
	.money_sale_percent {
		color: $product_price_color;
		text-align: center;
		background: none;
		font-size: 17px;
	}
}

#quick_view__availability {
	margin: 20px 0 0 0;
}
#quick_view__description {
	margin: 15px 0;
	p {
		font-size: 16px;
		line-height: 1.4em;
	}
}

#quick_view__form {
	margin: 15px 0 0 0;
	label {
		display: block;
	}
	.quantity_box {
		vertical-align: middle;
		height: 50px;
	}
	#quick_view__add {
		height: 50px;
		margin: 0px 10px 0 0;
		padding: 0 20px;
		font-size: 18px;
	}
}

#quick_view_colors,
#quick_view_size {
	display: flex;
	flex-wrap: wrap;
	margin: 12px 0;
	label {
		margin-right: 3px;
		margin-top: 5px;
		display: block;
		width: 100%;
		line-height: 1.4em;
	}
	.color_item {
		display: inline-block;
		vertical-align: top;
		width: 30px;
		height: 30px;
		cursor: pointer;
		border: 1px solid transparent;
		padding: 1px;
		@include border_radius(5px);
		margin: 5px 9px 0 0;
		span {
			background-color: #474747;
			border: 1px solid #ccc;
			display: block;
			width: 100%;
			height: 100%;
			@include border_radius(5px);
		}
	}
	.size_item {
		display: inline-block;
		vertical-align: baseline;
		margin: 5px 11px 0 0;
		background: $color_3;
		border: 1px solid $border_color;
		text-align: center;
		line-height: 30px;
		min-width: 30px;
		padding: 0 2px;
		cursor: pointer;
		@include border_radius(5px);
	}
	.current {
		border-color: $color_1;
	}
}




// 4.10 FANCYBOX COMMON STYLES ======================================================================================================================
.fancybox-image,
.fancybox-inner,
.fancybox-nav,
.fancybox-nav span,
.fancybox-outer,
.fancybox-skin,
.fancybox-tmp,
.fancybox-wrap,
.fancybox-wrap iframe,
.fancybox-wrap object {
	padding: 0;
	margin: 0;
	border: 0;
	outline: 0;
	vertical-align: top;
}
.fancybox-wrap {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 8020;
}
.fancybox-inner,
.fancybox-outer,
.fancybox-skin {
	position: relative;
}
.fancybox-skin {
	background: #f9f9f9;
	color: #444;
	text-shadow: none;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}
.fancybox-opened {
	z-index: 8030;
}
.fancybox-opened .fancybox-skin {
	-webkit-box-shadow: 0 10px 25px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0 10px 25px rgba(0, 0, 0, 0.5);
	box-shadow: 0 10px 25px rgba(0, 0, 0, 0.5);
}
.fancybox-inner {
	overflow: hidden;
}
.fancybox-type-iframe .fancybox-inner {
	-webkit-overflow-scrolling: touch;
}
.fancybox-error {
	color: #444;
	font: 14px/20px "Helvetica Neue", Helvetica, Arial, sans-serif;
	margin: 0;
	padding: 15px;
	white-space: nowrap;
}
.fancybox-iframe,
.fancybox-image {
	display: block;
	width: 100%;
	height: 100%;
}
.fancybox-image {
	max-width: 100%;
	max-height: 100%;
}
#fancybox-loading {
	position: fixed;
	top: 50%;
	left: 50%;
	margin-top: -22px;
	margin-left: -22px;
	background: url(data:image/png;base64,);
	opacity: 0.8;
	cursor: pointer;
	z-index: 8060;
}
.fancybox-close,
.fancybox-nav,
.fancybox-nav span {
	cursor: pointer;
	z-index: 8040;
	position: absolute;
}
#fancybox-loading div {
	width: 44px;
	height: 44px;
	background: url(fancybox_loading.gif) center center no-repeat;
}
.fancybox-close {
	top: -12px;
	right:-12px;
	width: 36px;
	height: 36px;
}
.fancybox-nav {
	top: 0;
	width: 40%;
	height: 100%;
	text-decoration: none;
	background: url(blank.gif);
	-webkit-tap-highlight-color: transparent;
}
.fancybox-prev {
	left: 0;
}
.fancybox-next {
	right: 0;
}
.fancybox-nav span {
	top: 50%;
	width: 36px;
	height: 34px;
	margin-top: -18px;
}
.fancybox-prev span {
	left: 5px;
}
.fancybox-next span {
	right: 5px;
}
.fancybox-nav:hover span {
	visibility: visible;
}
.fancybox-tmp {
	position: absolute;
	top: -99999px;
	left: -99999px;
	visibility: hidden;
	max-width: 99999px;
	max-height: 99999px;
	overflow: visible !important;
}
.fancybox-lock,
.fancybox-lock body {
	overflow: hidden !important;
}
.fancybox-lock {
	width: auto;
}
.fancybox-lock-test {
	overflow-y: hidden !important;
}
.fancybox-overlay {
	width: 100% !important;
	height: 100% !important;
	position: absolute;
	top: 0;
	left: 0;
	overflow: hidden;
	display: none;
	z-index: 8010;
	background: rgba(0, 0, 0, 0.7);
}
.fancybox-overlay-fixed {
	position: fixed;
	bottom: 0;
	right: 0;
}
.fancybox-lock .fancybox-overlay {
	overflow: auto;
	overflow-y: scroll;
}
.fancybox-title {
	visibility: hidden;
	font: 400 13px/20px "Helvetica Neue", Helvetica, Arial, sans-serif;
	position: relative;
	text-shadow: none;
	z-index: 8050;
}
.fancybox-opened .fancybox-title {
	visibility: visible;
}
.fancybox-title-float-wrap {
	position: absolute;
	bottom: 0;
	right: 50%;
	margin-bottom: -35px;
	z-index: 8050;
	text-align: center;
}
.fancybox-title-float-wrap .child {
	display: inline-block;
	margin-right: -100%;
	padding: 2px 20px;
	background: 0 0;
	background: rgba(0, 0, 0, 0.8);
	-webkit-border-radius: 15px;
	-moz-border-radius: 15px;
	border-radius: 15px;
	text-shadow: 0 1px 2px #222;
	color: #fff;
	font-weight: 700;
	line-height: 24px;
	white-space: nowrap;
}
.fancybox-title-outside-wrap {
	position: relative;
	margin-top: 10px;
	color: #fff;
}
.fancybox-title-inside-wrap {
	padding-top: 10px;
}
.fancybox-title-over-wrap {
	position: absolute;
	bottom: 0;
	left: 0;
	color: #fff;
	padding: 10px;
	background: #000;
	background: rgba(0, 0, 0, 0.8);
}

.fancybox-skin {
	padding: 15px;
	background: $color_3;
	@include border_radius(0);
}

.fancybox-close {
	width: 24px;
	height: 24px;
	display: block;
	border: none;
	background: black;
	border-radius: 5px;
	&:before {
		content: "";
		display: block;
		width: 13px;
		height: 1px;
		background: $color_3;
		position: absolute;
		top: 12px;
		right: 5px;
		@include transform(rotate(45deg));
	}
	&:after {
		content: "";
		display: block;
		width: 13px;
		height: 1px;
		background: $color_3;
		position: absolute;
		top: 12px;
		right: 5px;
		@include transform(rotate(135deg));
	}
	&:hover {
		background: #eee;
		&:before, &:after {
			background: black;
		}
	}
}

.fancybox-nav {
	span {
		width: 36px;
		height: 36px;
		display: block;
		margin: -18px 0 0 0;
		background: #444;
		border: none;
		@include border_radius(50%);
		@include box_shadow(0 0 10px 0 rgba(0, 0, 0, 0.6));
		@include transition(0.3s, background, border-color, color);

		&:before {
			display: block;
			font: 12px/36px "FontAwesome";
			color: $color_4;
			text-align: center;
		}
	}

	&:hover > span {
		background: #666;
	}
}

.fancybox-prev span:before {
	content: "\f053";
	padding: 0 2px 0 0;
}
.fancybox-next span:before {
	content: "\f054";
	padding: 0 0 0 2px;
}

.fancybox_alert {
	padding: 0 !important;
	text-align: center;
	.alert {
		margin: 0;
		padding: 40px;
		font-size: 14px;
	}

	.btn {
		margin: 15px 0 0 0;
		text-decoration: none;
	}
	.btn ~ .btn {
		margin-left: 15px !important;
	}
}


// 4.11 ADD TO CART MESSAGE =========================================================================================================================
#cart_added {
	width: 380px;
	height: 366px;
	.cart_added__row {
		display: flex;
		flex-wrap: wrap;
		padding: 15px 0 10px;
	}
	h4 {
		margin: -2px 0 0;
		color: #349E53;
		font-size: 15px;
		letter-spacing: -0.015em;
		font-weight: bold;
		svg {
			display: inline-block;
			vertical-align: bottom;
		}
	}

	&.cart_error {
		max-width: 380px;
	}
}

.cart_added__1 {
	width: 140px;
	height: 100%;
	text-align: center;
	vertical-align: top;
	line-height: 0;
	img {
		max-width: 100%;
	}
}

.cart_added__2 {
	width: 240px;
	padding: 0 0 0 29px;
	.product_name {
		display: block;
		line-height: 21px;
		letter-spacing: 0.015em;
	}
	#cart_added__quantity {
		margin: 5px 0 0 0;
		font-size: 13px;
	}
	.product_price {
		font-size: 17px;
		font-weight: bold;
	}
	#cart_added__total_quantity {
		margin: 8px 0 0;
		font-size: 13px;
		letter-spacing: -0.015em;
	}
	#cart_added__total_price {
		font-size: 13px;
		margin: 7px 0 5px;
		span {
			font-weight: bold;
		}
	}
}

.cart_added__3 {
	width: 100%;
	padding-top: 15px;
	.btn {
		width: 100%;
		margin: 12px 0 0;
	}
}

// 4.12 NEWSLETTER POPUP ============================================================================================================================
#newsletter_popup {
	display: flex;
	justify-content: center;
	align-items: center;
	height: 400px;
	.newsletter_popup_text {
		text-align: center;
		h4 {
			margin-bottom: 15px;
		}
		input {
			height: 40px;
		}
		p {
			margin-bottom: 10px;
		}
		.btn {
			margin-top: 15px;
		}
	}
}



