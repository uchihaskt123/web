.template_page_sections_all {
	padding: 8px 0 0 0;
	.page_heading {
		margin-top: 15px;
	}
	.page_description {
		text-align: center;
		letter-spacing: -0.74px;
		margin-top: 4px;
	}
	hr {
		color: $border-color;
		margin: 32px 0 28px;
	}

	.section {
		margin-bottom: 60px;
	}

	.section_banners_demo_1 {margin-bottom: 20px;}

	.section_banners_demo_1, .section_banners_demo_2 {
		.banner_100 {
			.img_placeholder__wrap {
				padding: 38.6745% 0 0 0;
			}
			h2 {
				font-size: 60px;
			}
			.btn {
				min-width: 126px;
				margin: 17px 3px 7px;
			}
		}
		.banner_66 {
			.img_placeholder__wrap {
				padding: 58.497% 0 0 0;
			}
		}
		.banner_50 {
			.img_placeholder__wrap {
				padding: 78.652% 0 0 0;
			}
			.banner_text {
				margin-bottom: 25px;
			}
		}
		.banner_33 {
			.img_placeholder__wrap {
				padding: 120.002% 0 0 0;
			}
		}
		.section_heading {
			margin-bottom: 35px;
		}
		.banner_item {
			position: relative;
			line-height: 0;
		}
		.hover_image {
			overflow: hidden;
			img {
				@include transition(.4s,transform);
			}
			&:hover {
				img {
					transform: scale(1.05);
				}
			}
		}
		.btn {
			padding: 10px 20px;
		}
		.caption_text {
			display: block;
		}
		.banner_caption {
			line-height: 1em;
			position: absolute;
			top: 0;
			bottom: 0;
			left: 0;
			right: 0;
			padding: 15px 35px;
			display: flex;
			justify-content: center;
			align-items: center;
			text-align: center;

			h2 {
				text-transform: uppercase;
				margin: 15px 0;
				line-height: 1em;
				letter-spacing: -0.05em;
			}
			h4 {
				font-size: 16px;
				letter-spacing: -0.05em;
			}
			&.style_minimal {
				text-align: left;
				padding: 30px 50px;
				.caption_text {
					width: 100%;
					height: 100%;
					display: flex;
					flex-direction: column;
					align-items: flex-start;
					justify-content: flex-end;
				}
				.banner_text {
					position: relative;
					font-weight: bold;
					display: inline-block;
					color: $heading_font_color;
					&:after {
						content: '';
						position: absolute;
						bottom: -3px;
						left: 0;
						width: 0;
						height: 2px;
						background: $heading_font_color;
						@include transition(.4s);
					}
				}
				.btn {
					margin-bottom: 19px;
				}
				.inverted {
					.banner_text {
						color: $color_3;
					}
					.banner_text:after {
						background: $color_3;
					}
					.btn {
						color: #3c5ebe;
						background: $color_3;
						border-color: $color_3;
						&:hover {
							background: #3c5ebe;
							border-color: #3c5ebe;
							color: #ffffff;
						}
					}
				}
			}
		}
		.style_minimal a.caption_text:hover .banner_text:after {
			width: 100%;
		}
	}

	.section_collection-list_demo {margin-top: 20px;}

	.index-section-slideshow-demo {margin-top: 20px;}

	.index_section_testimonials_demo_1, .index_section_testimonials_demo_2 {margin-top: 42px;}
}

.wrap_triger {
	position: relative;
	padding-right: 25px;

	&:after {
		position: absolute;
		content: '\f067';
		font: 14px/1em 'FontAwesome';
		right: 5px;
		top: 50%;
		display: block;
		margin-top: -7px;
		cursor: pointer;
		@include transition(.3s,transform);
	}
