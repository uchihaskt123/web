		    @include transition(0.1s);
		    text-transform: capitalize;
		    font-weight: bold;
		    font-size: 20px;
		    margin: 15px 0 5px;
			letter-spacing: -0.05em;
	  	}
	  	svg {
		    fill: $color_3;
		    @include transition(0.3s);
	  	}
  
	  	&:hover, &:focus {
			background-color: $color_2;
			color: $color_3;
		    svg, path {
		      fill: $color_3;
		    }
			p {
				color: $color_3;
			}
	  	}
	  	a:hover {
	  		opacity: 1;
	  	}
	  	.logo_wrap {
	    	padding: 5px;
	  	}

	  	.img_placeholder__large {
	    	.icon {
	      		margin-top: 10%;
	    	}
	    	p {
	      		margin-bottom: 10%;
	    	}
	    	.inline {
	    	}
	  	}
	  	&.inline .img_placeholder__large {
	    	.icon {
	      		margin-top: 0;
	    	}
	    	p {
	      		margin-bottom: 0;
	    	}
	  	}	
	}

	.img_placeholder__wrap {
		width: 100%;
		@include transition(0.3s);
	}
	.img_placeholder__xsmall {
	  	padding: 45.89% 0 0 0;
	}
	.img_placeholder__small {
	  	padding: 70% 0 0 0;
	}
	.img_placeholder__medium {
	  	padding: 72.95% 0 0 0;
	}
	.img_placeholder__large {
	  	padding: 77.65% 0 0 0;
	}

	a {
	  	@include transition(0.3s, opacity);
	}

	a:hover {
	 	opacity: 0.7;
	}
	
}


