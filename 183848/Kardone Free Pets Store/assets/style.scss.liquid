		.title_table {color: $color_1;font-size: 16px;font-weight: 300;}
		.wrap_content {display: none;}
		.wrap_triger {margin: 10px auto;}
	}

	.type_tile {
		.product__img_wrap {
			display: flex;
			flex-wrap: wrap;

			.product__img_item {
				width: 50%;
				padding: 5px;
				position: relative;
				max-width: 375px;
				line-height: 0;
				&:hover {
					.product__img_zoom {
						opacity: 1;
					}
				}
				img {
					border: 1px solid $border_color;
					border-radius: 5px;
					max-width: 100%;
				}
			}
			.product__img_zoom {
				position: absolute;
				top: 50%;
				left: 50%;
				margin: -22px 0 0 -22px;
				display: block;
				width: 44px;
				height: 44px;
				text-align: center;
				line-height: 44px;
				background: $color_4;
				border-radius: 5px;
				cursor: pointer;
				opacity: 0;
				@include transition(.2s, opacity);

				svg {
					margin-top: 11px;
				}
			}
		}
	}

	.type_carousel_big {
		.row {
			width: 100%;
		    padding-right: 10px;
		    padding-left: 10px;
		    margin-right: auto;
		    margin-left: auto;
		}
