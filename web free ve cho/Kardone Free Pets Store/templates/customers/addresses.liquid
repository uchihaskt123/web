{% paginate customer.addresses by 5 %}

<div class="template_customer template_customer__addresses">
	<div class="container">
		<h1 class="page_heading">{{ page_title }} <a href="/account" title="{{ 'customer.addresses.back_to_account' | t }}">
			<svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
				<path d="M9 0H20V20H9V18H18V2H9V0Z" fill="#222222"/>
				<path d="M3.31844 9H12V11H3.25147L6 13.7156L4.85714 15L0 10.0458L4.85714 5L6 6.28441L3.31844 9Z" fill="#222222"/>
			</svg>
		</a></h1>


		{% comment %}
		NEW ADDRESS *********************************************************************************************************************************
		{% endcomment %}
		<div class="account_section account_address__add">
			<h4>{{ 'customer.addresses.new_address' | t }}
				<a id="address_add__link" class="link_add" href="#" title="{{ 'customer.addresses.address_add' | t }}">
					<svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
						<path d="M7 0H5V5H0V7H5V12H7V7H12V5H7V0Z" fill="#222222"/>
					</svg>
				</a>
				<a id="address_add__close" class="link_close" href="#" title="{{ 'customer.addresses.address_close' | t }}">
					<svg width="10" height="10" viewBox="0 0 10 10" fill="none" xmlns="http://www.w3.org/2000/svg">
						<path fill-rule="evenodd" clip-rule="evenodd" d="M8.71558 0L10 1.14286L5.99302 5L10 8.85714L8.71558 10L4.95417 6.28571L1.28442 10L0 8.85714L3.9342 5L0 1.14286L1.28442 0L4.95417 3.71429L8.71558 0Z" fill="#222222"/>
					</svg>
				</a>
			</h4>

			<div id="address_add" class="form form-horizontal">
				{% form 'customer_address', customer.new_address %}
					<div class="form-group">
						<label class="control-label" for="address_first_name_new">{{ 'customer.addresses.form_first_name' | t }}</label>
						<div class=""><input type="text" id="address_first_name_new" class="form-control" name="address[first_name]" value="{{ form.first_name }}"></div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_last_name_new">{{ 'customer.addresses.form_last_name' | t }}</label>
						<div class=""><input type="text" id="address_last_name_new" class="form-control" name="address[last_name]" value="{{ form.last_name }}"></div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_company_new">{{ 'customer.addresses.form_company' | t }}</label>
						<div class=""><input type="text" for="address_company_new" class="form-control" name="address[company]" value="{{ form.company }}"></div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_address1_new">{{ 'customer.addresses.form_address_1' | t }}</label>
						<div class=""><input type="text" id="address_address1_new" class="form-control" name="address[address1]" value="{{ form.address1 }}"></div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_address2_new">{{ 'customer.addresses.form_address_2' | t }}</label>
						<div class=""><input type="text" id="address_address2_new" class="form-control" name="address[address2]" value="{{ form.address2 }}"></div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_city_new">{{ 'customer.addresses.form_city' | t }}</label>
						<div class="">
							<input type="text" id="address_city_new" class="form-control" name="address[city]" value="{{ form.city }}">
						</div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_country_new">{{ 'customer.addresses.form_country' | t }}</label>
						<div class="">
							<select class="form-control" id="address_country_new" name="address[country]" data-default="{{ form.country }}">{{ country_option_tags }}</select>
						</div>
					</div>

					<div id="address_province_container_new" style="display:none" class="form-group">
						<label class="control-label" for="address_province_new">{{ 'customer.addresses.form_province' | t }}</label>
						<div class="">
							<select class="form-control" id="address_province_new" name="address[province]" data-default="{{ form.province }}"></select>
						</div>
					</div>

					<div class="form-group">
						<label class="control-label " for="address_zip_new">{{ 'customer.addresses.form_zip' | t }}</label>
						<div class="">
							<input type="text" id="address_zip_new" class="form-control" name="address[zip]" value="{{ form.zip }}">
						</div>
					</div>

					<div class="form-group">
						<label class="control-label" for="address_phone_new">{{ 'customer.addresses.form_phone' | t }}</label>
						<div class="">
							<input type="text" id="address_phone_new" class="form-control" name="address[phone]" value="{{ form.phone }}">
						</div>
					</div>

					<div class="form-group">
						<div class="">
							<label for="address_default_address_new">{{ form.set_as_default_checkbox }} {{ 'customer.addresses.form_set_default_address' | t }}</label>
						</div>
					</div>

					<div class="form-group">
						<div class="">
							<button class="btn" type="submit">{{ 'customer.addresses.form_add_address' | t }}</button>
							
						</div>
					</div>
				{% endform %}
			</div>
		</div>


{% if customer.addresses.size > 0 %}
		{% comment %}
		DEFAULT ADDRESS *****************************************************************************************************************************
		{% endcomment %}
		<div class="account_section account_address__item" id="account_address__{{ customer.default_address.id }}">
			<h4>{{ customer.default_address.name }} ({{ 'customer.addresses.default_address' | t }})
				<a class="link_delete" href="#" title="{{ 'customer.addresses.address_delete' | t }}: {{ customer.default_address.id }}">
