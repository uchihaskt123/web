					</svg>
					<span>{{ customer.default_address.phone }}</span>
				</p>
			{% endif %}

			<div class="account_address__edit form-horizontal">
				{% form 'customer_address', customer.default_address %}
					<div class="form-group">
						<label class="control-label" for="address_first_name_{{ form.id }}">{{ 'customer.addresses.form_first_name' | t }}</label>
						<div>
							<input type="text" id="address_first_name_{{ form.id }}" name="address[first_name]" value="{{ form.first_name }}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_last_name_{{ form.id }}">{{ 'customer.addresses.form_last_name' | t }}</label>
						<div>
							<input type="text" id="address_last_name_{{ form.id }}" name="address[last_name]" value="{{form.last_name}}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_company_{{ form.id }}">{{ 'customer.addresses.form_company' | t }}</label>
						<div>
							<input type="text" id="address_company_{{ form.id }}" name="address[company]" value="{{form.company}}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_address1_{{ form.id }}">{{ 'customer.addresses.form_address_1' | t }}</label>
						<div>
							<input type="text" id="address_address1_{{ form.id }}" name="address[address1]" value="{{form.address1}}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_address2_{{ form.id }}">{{ 'customer.addresses.form_address_2' | t }}</label>
						<div>
							<input type="text" id="address_address2_{{ form.id }}" name="address[address2]" value="{{form.address2}}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_city_{{ form.id }}">{{ 'customer.addresses.form_city' | t }}</label>
						<div>
							<input type="text" id="address_city_{{ form.id }}" name="address[city]" value="{{form.city}}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_country_{{ form.id }}">{{ 'customer.addresses.form_country' | t }}</label>
						<div>
							<select id="address_country_{{ form.id }}" name="address[country]" data-default="{{form.country}}">{{ country_option_tags }}</select>
						</div>
					</div>
					<div class="form-group" id="address_province_container_{{ form.id }}" style="display:none">
						<label class="control-label" for="address_province_{{ form.id }}">{{ 'customer.addresses.form_province' | t }}</label>
						<div>
							<select id="address_province_{{ form.id }}" name="address[province]" data-default="{{form.province}}"></select>
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_zip_{{ form.id }}">{{ 'customer.addresses.form_zip' | t }}</label>
						<div>
							<input type="text" id="address_zip_{{ form.id }}" name="address[zip]" value="{{ form.zip }}">
						</div>
					</div>
					<div class="form-group">
						<label class="control-label" for="address_phone_{{ form.id }}">{{ 'customer.addresses.form_phone' | t }}</label>
						<div>
							<input type="text" id="address_phone_{{ form.id }}" name="address[phone]" value="{{ form.phone }}">
						</div>
					</div>
					<div class="form-group">
						<div>
							<button type="submit" class="btn">{{ 'customer.addresses.form_update_address' | t }}</button>
							
						</div>
					</div>
				{% endform %}
			</div>
		</div>


		{% comment %}
		OTHER ADDRESSES *****************************************************************************************************************************
