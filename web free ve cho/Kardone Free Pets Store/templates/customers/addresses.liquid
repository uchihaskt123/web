						<svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
							<path d="M19.9005 18.8945C19.801 17.9899 19.602 17.0854 19.204 16.1809C18.7065 14.9749 18.01 13.8693 17.0149 12.9648C16.1194 12.0603 15.0249 11.2563 13.8308 10.7538C11.4428 9.74874 8.65672 9.74874 6.16915 10.7538C4.97512 11.2563 3.8806 11.9598 2.98507 12.8643C2.08955 13.7688 1.29353 14.8744 0.79602 16.0804C0.497512 16.9849 0.199005 17.9899 0.0995025 18.8945L0 20H20L19.9005 18.8945ZM2.28856 17.9899C2.38806 17.5879 2.48756 17.2864 2.68657 16.8844C3.08458 15.8794 3.68159 15.0754 4.37811 14.2714C5.17413 13.5678 5.97015 12.9648 6.96517 12.5628C8.95522 11.7588 11.1443 11.7588 13.0348 12.5628C14.0299 12.9648 14.9254 13.5678 15.6219 14.2714C16.3184 14.9749 16.9154 15.8794 17.3134 16.8844C17.4129 17.1859 17.6119 17.5879 17.7114 17.9899H2.28856Z" fill="#222222"/>
							<path d="M12.9 6.88199C13.6 6.18634 14 5.19255 14 4C14 2.80745 13.6 1.91304 12.9 1.11801C11.3 -0.372671 8.7 -0.372671 7.2 1.11801C6.4 1.91304 6 2.90683 6 4C6 5.09317 6.4 6.08696 7.2 6.88199C8.7 8.37267 11.3 8.37267 12.9 6.88199ZM8.6 2.6087C9.3 1.91304 10.7 1.91304 11.4 2.6087C11.8 3.00621 12 3.50311 12 4C12 4.49689 11.8 4.99379 11.4 5.3913C10.7 6.08696 9.3 6.08696 8.6 5.3913C8.2 5.09317 8 4.59627 8 4C8 3.40373 8.2 2.90683 8.6 2.6087Z" fill="#222222"/>
						</svg>
						<span>{{ address.name }}</span>
					</p>
				{% endif %}

				<p class="address_item address_item__location">
					<svg width="24" height="22" viewBox="0 0 24 22" fill="none" xmlns="http://www.w3.org/2000/svg">
						<path d="M19.7 21H14.2V14.5H9.8V21H4.3V10H1L12 1L23 10H19.7V21ZM15.3 20H18.6V9H20.14L12 2.35L3.86 9H5.4V20H8.7V13.5H15.3V20Z" fill="#222222" stroke="black" stroke-width="0.5"/>
					</svg>
					{% if address.address1.size > 0 %}<span>{{ address.address1 }}</span>{% endif %}
					{% if address.address2.size > 0 %}<span>{{ address.address2 }}</span>{% endif %}
					{% if address.city.size > 0 %}<span>{{ address.city }}</span>{% endif %}
					{% if address.province.size > 0 %}<span>{{ address.province }}</span>{% endif %}
					{% if address.zip.size > 0 %}<span>{{ address.zip }}</span>{% endif %}
					{% if address.country.size > 0 %}<span>{{ address.country }}</span>{% endif %}
				</p>

				{% if address.phone.size > 0 %}
					<p class="address_item address_item__phone">
						<svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
							<path d="M19.9005 18.8945C19.801 17.9899 19.602 17.0854 19.204 16.1809C18.7065 14.9749 18.01 13.8693 17.0149 12.9648C16.1194 12.0603 15.0249 11.2563 13.8308 10.7538C11.4428 9.74874 8.65672 9.74874 6.16915 10.7538C4.97512 11.2563 3.8806 11.9598 2.98507 12.8643C2.08955 13.7688 1.29353 14.8744 0.79602 16.0804C0.497512 16.9849 0.199005 17.9899 0.0995025 18.8945L0 20H20L19.9005 18.8945ZM2.28856 17.9899C2.38806 17.5879 2.48756 17.2864 2.68657 16.8844C3.08458 15.8794 3.68159 15.0754 4.37811 14.2714C5.17413 13.5678 5.97015 12.9648 6.96517 12.5628C8.95522 11.7588 11.1443 11.7588 13.0348 12.5628C14.0299 12.9648 14.9254 13.5678 15.6219 14.2714C16.3184 14.9749 16.9154 15.8794 17.3134 16.8844C17.4129 17.1859 17.6119 17.5879 17.7114 17.9899H2.28856Z" fill="#222222"/>
							<path d="M12.9 6.88199C13.6 6.18634 14 5.19255 14 4C14 2.80745 13.6 1.91304 12.9 1.11801C11.3 -0.372671 8.7 -0.372671 7.2 1.11801C6.4 1.91304 6 2.90683 6 4C6 5.09317 6.4 6.08696 7.2 6.88199C8.7 8.37267 11.3 8.37267 12.9 6.88199ZM8.6 2.6087C9.3 1.91304 10.7 1.91304 11.4 2.6087C11.8 3.00621 12 3.50311 12 4C12 4.49689 11.8 4.99379 11.4 5.3913C10.7 6.08696 9.3 6.08696 8.6 5.3913C8.2 5.09317 8 4.59627 8 4C8 3.40373 8.2 2.90683 8.6 2.6087Z" fill="#222222"/>
						</svg>
						<span>{{ address.phone }}</span>
					</p>
				{% endif %}

				<div class="account_address__edit form-horizontal">
					{% form 'customer_address', address %}
						<div class="form-group">
							<label class="control-label" for="address_first_name_{{ form.id }}">{{ 'customer.addresses.form_first_name' | t }}</label>
							<div>
								<input type="text" id="address_first_name_{{ form.id }}" name="address[first_name]" value="{{ form.first_name }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_last_name_{{ form.id }}">{{ 'customer.addresses.form_last_name' | t }}</label>
							<div>
								<input type="text" id="address_last_name_{{ form.id }}" name="address[last_name]" value="{{ form.last_name }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_company_{{ form.id }}">{{ 'customer.addresses.form_company' | t }}</label>
							<div>
								<input type="text" id="address_company_{{ form.id }}" name="address[company]" value="{{ form.company }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_address1_{{ form.id }}">{{ 'customer.addresses.form_address_1' | t }}</label>
							<div>
								<input type="text" id="address_address1_{{ form.id }}" name="address[address1]" value="{{ form.address1 }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_address2_{{ form.id }}">{{ 'customer.addresses.form_address_2' | t }}</label>
							<div>
								<input type="text" id="address_address2_{{ form.id }}" name="address[address2]" value="{{ form.address2 }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_city_{{ form.id }}">{{ 'customer.addresses.form_city' | t }}</label>
							<div>
								<input type="text" id="address_city_{{ form.id }}" name="address[city]" value="{{ form.city }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_country_{{ form.id }}">{{ 'customer.addresses.form_country' | t }}</label>
							<div>
								<select id="address_country_{{ form.id }}" name="address[country]" data-default="{{ form.country }}">{{ country_option_tags }}</select>
							</div>
						</div>
						<div class="form-group" id="address_province_container_{{ form.id }}" style="display: none;">
							<label for="address_province_{{ form.id }}">{{ 'customer.addresses.form_province' | t }}</label>
							<div>
								<select id="address_province_{{ form.id }}" name="address[province]" data-default="{{ form.province }}"></select>
							</div>
						</div>
						<div class="form-group">
							<label for="address_zip_{{ form.id }}">{{ 'customer.addresses.form_zip' | t }}</label>
							<div>
								<input type="text" id="address_zip_{{ form.id }}" name="address[zip]" value="{{ form.zip }}">
							</div>
						</div>
						<div class="form-group">
							<label for="address_phone_{{ form.id }}">{{ 'customer.addresses.form_phone' | t }}</label>
							<div>
								<input type="text" id="address_phone_{{ form.id }}" name="address[phone]" value="{{ form.phone }}">
							</div>
						</div>
						<div class="form-group">
							<div>
								<label for="address_default_address_{{ form.id }}">{{ form.set_as_default_checkbox }} {{ 'customer.addresses.form_set_default_address' | t }}</label></div>
						</div>
						<div class="form-group">
							<div>
								<button class="btn" type="submit">Update address</button>
								
							</div>
						</div>
					{% endform %}
				</div>
			</div>
			{% endif %}
		{% endfor %}
{% endif %}


		{% if paginate.pages > 1 %}
			{% assign pagination_items = 'address(es)' %}
			{% include 'snippet-pagination' with pagination_items %}
		{% endif %}
	</div>
</div>

<script>
	theme.customerAddresses = '{{ customer.addresses | json }}';
</script>

{% endpaginate %}

{% section 'template-addresses' %}